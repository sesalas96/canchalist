{
  "name": "my-cci-api",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "build": "tsc && tsc-alias",
    "start": "node dist/app.js",
    "develop": "nodemon",
    "dev": "ts-node -r tsconfig-paths/register src/app.ts",
    "test": "jest",
    "new-core": "rm package-lock.json && rm -rf node_modules && npm update && npm run develop"
  },
  "dependencies": {
    "@aws-sdk/client-rds": "^3.624.0",
    "@aws-sdk/client-s3": "^3.620.0",
    "@aws-sdk/credential-providers": "^3.624.0",
    "@aws-sdk/rds-signer": "^3.624.0",
    "@jm18457/kafkajs-msk-iam-authentication-mechanism": "^3.1.2",
    "@mule-migration/core": "^0.0.2",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-rate-limit": "^7.3.1",
    "express-validator": "^7.1.0",
    "fast-xml-parser": "^4.4.0",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "kafkajs": "^2.2.4",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.12.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/js-yaml": "^4.0.9",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.6",
    "@types/sax": "^1.2.7",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "lint-staged": "^10.5.4",
    "nodemon": "3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "tsc-alias": "^1.8.10"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "_moduleAliases": {
    "@src": "src",
    "@config": "dist/config.js"
  }
}
