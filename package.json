    "express-rate-limit": "^7.3.1",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "kafkajs": "^2.2.4",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
