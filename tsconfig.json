{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "@src/*": ["src/*"],
        "@config": ["src/config.ts"],
      },
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist"
    },
    "include": ["src/**/*"]
  }  